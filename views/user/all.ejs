<div class="content-wrapper">
	<!-- Content Header (Page header) -->
	<section class="content-header">
		<h2>All Users <a class="leftSpace" href="/admin/users/new"><i class="fa fa-plus"></i></a></h2>
		<ol class="breadcrumb">
			<li><a href="/dashboard"><i class="fa fa-dashboard"></i> Home</a></li>
			<li class="active">Users</li>
		</ol>
	</section>

	<!-- Main content -->
	<section class="content">
		<div class="box">
			<div class="box-body">
				<table id="allUsers" class="table table-bordered table-striped">
					<thead>
						<tr>
							<th>#</th>
							<th>Title</th>
							<th>Status</th>
							<th>Role</th>
							<th>User Since</th>
							<th>Action</th>
						</tr>
					</thead>
					<tbody>
						<% for(var u in allUsers) { %>
							<% for(var user in allUsers[u]) { %>
								<tr>
									<td><%= allUsers[u][user].id %></td>
									<td><a href="/admin/users/<%= allUsers[u][user].id %>/edit"><%= allUsers[u][user].first_name %> <%= allUsers[u][user].last_name %></a></td>
									<td><%= allUsers[u][user].status %></td>
									<td><%= allUsers[u][user].role.title %></td>
									<td class="moment" datetime="<%= allUsers[u][user].createdAt %>"><%= allUsers[u][user].createdAt %></td>
									<td>
										<span class="action">
											<a data-toggle="tooltip" title="Edit" href="/admin/users/<%= allUsers[u][user].id %>/edit"><i class="fa fa-pencil"></i></a> | <a data-toggle="tooltip" title="Delete" href=""><i class="fa fa-trash"></i></a>
										</span>
									</td>
								</tr>
							<% } %>
						<% } %>
					</tbody>
				</table>
			</div>
		</div>
	</section>